{
  "name": "quietjs-bundle",
  "version": "0.1.1",
  "description": "A utility to bundle quiet-js in one file",
  "main": "index.js",
  "files": [
    "bundler.mjs",
    "template.index.d.ts",
    "LICENSE-3RD-PARTY"
  ],
  "scripts": {
    "bundle": "node bundler.mjs",
    "postinstall": "run-s bundle",
    "testbundle": "run-s bundle testbundle:browserify",
    "testbundle:browserify": "browserify test/index.js -o test/b.js"
  },
  "keywords": [
    "bundle",
    "bundler",
    "libquiet",
    "quietjs",
    "quiet.js",
    "quiet-js",
    "quiet"
  ],
  "author": "Edgar Onghena",
  "license": "MIT",
  "repository": "https://github.com/edgarogh/quietjs-bundle",
  "dependencies": {
    "base64-arraybuffer": "^1.0.2",
    "chalk": "^5.0.0",
    "node-fetch": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.14.5"
  },
  "devDependencies": {
    "browserify": "^17.0.0"
  }
}
